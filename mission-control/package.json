{
  "name": "Sample",
  "plan": "mission-control",
  "version": "1.2.8",
  "description": "Manage users, view metrics, track events, collect feedback and log errors in one central dashboard",
  "main": "server.js",
  "engines": {
    "node": ">=20.5.0"
  },
  "dependencies": {
    "@types/node": "^22.6.1",
    "axios": "^1.2.3",
    "bcrypt": "^5.1.0",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-mongo-sanitize": "2.2.0",
    "express-rate-limit": "^6.7.0",
    "form-data": "^4.0.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "5.1.0",
    "mongoose": "7.0.3",
    "mysql2": "3.2.0",
    "postmark": "^4.0.5",
    "react-redux": "^9.1.2",
    "stripe": "11.7.0",
    "typescript": "^5.5.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  },
  "scripts": {
    "setup": "node bin/installcheck && npm install && npm link && cd client && npm install && cd ../ && npm run dev",
    "client": "cd client && npm start",
    "client-build": "cd client && npm i && npm run build",
    "server": "nodemon --watch src --ext ts,js,json --exec ts-node ./src/server.ts",
    "node": "node dist/server.js",
    "build": "tsc",
    "cleanup": "node bin/cleanup",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "dev": "concurrently \"npm run server\" \"sleep 10 && npm run client\""
  },
  "nodemonConfig": {
    "ignore": [
      "client/",
      "config/",
      "server.js",
      "knexfile.js"
    ]
  },
  "repository": {
    "type": "git"
  },
  "author": "Hefler",
  "license": "PRIVATE",
  "private": true,
  "homepage": "https://Sample.app"
}
