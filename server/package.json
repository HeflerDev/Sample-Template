  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/config": "^3.3.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.5",
    "@types/passport": "^1.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.56.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "setup": "node bin/installcheck && npm install && npm link; node bin/clientcheck && cd ../client && npm install; cd ../server && node bin/appcheck && cd ../app && npm install && cd ../server && node bin/setup.js; npm run dev",
    "cleanup": "node bin/cleanup",
    "client": "cd ../client && npm start",
    "server": "nodemon --watch src --ext ts,tsx --exec ts-node --transpile-only src/server.ts",
    "prod": "node --max-old-space-size=512 dist/server.js",
    "app": "cd ../app && expo start",
    "dev": "node bin/start",
    "test": "NODE_ENV=test mocha --require ts-node/register test/run.ts --exit",
    "eslint": "eslint --config ./.eslintrc.js ./src/**/*.ts",
    "eslint:fix": "eslint --config ./.eslintrc.js ./src/**/*.ts --fix",
    "test:coverage": "NODE_ENV=test nyc mocha --require ts-node/register test/run.ts --exit",
    "heroku-postbuild": "node bin/clientcheck && cd client && npm install && npm run build"
  },
  "nodemonConfig": {
    "ignore": [
      "client/",
      "config/",
      "server.js",
      "knexfile.js"
    ]
  },
  "repository": {
    "type": "git"
  },
  "author": "Hefler",
  "license": "PRIVATE",
  "private": true,
  "homepage": "https://Sample.app"
}
