    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "cryptr": "^6.2.0",
    "dotenv": "^16.0.3",
    "expo-server-sdk": "^3.7.0",
    "express": "^4.18.2",
    "express-mongo-sanitize": "2.2.0",
    "express-rate-limit": "^6.7.0",
    "form-data": "^4.0.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.3",
    "mongodb": "5.1.0",
